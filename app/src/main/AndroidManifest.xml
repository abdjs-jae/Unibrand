<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="unibrand.unibrand.unibrandapp">

    <!-- Internet permission -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".UnibrandVideoActivity"
            android:screenOrientation="sensorLandscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".MedicineActivity"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".CorporateVideoActivity"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Bioequivalence"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Atorvast"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Glicla"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Felodipin"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Azithro"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Levofloxin"
            android:screenOrientation="sensorLandscape" />
        <activity
            android:name=".Levocet"
            android:screenOrientation="sensorLandscape" />
        <activity android:name=".RegisterActivity"></activity>
    </application>

</manifest>